function removeDup(arr) {
  const uniqueArr = [];
  const seenValues = {}; 

  for (const item of arr) {
    if (!seenValues[item]) { 
      uniqueArr.push(item);
      seenValues[item] = true; 
    }
  }

  return uniqueArr;
}

const originalArray = [1, 4, 2, 3, 4, 1, 5];
const uniqueArray = removeDup(originalArray);
console.log(uniqueArray); 
